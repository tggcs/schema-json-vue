{
  "name": "schema-json-vue",
  "version": "1.0.15",
  "main": "./lib/schema-json-vue.common.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --dest lib --name schema-json-vue src/index.js",
    "build:demo": "vue-cli-service build --target app --dest dist --name schema-json-vue-demo example/main.js",
    "lint": "vue-cli-service lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tggcs/schema-json-vue.git"
  },
  "dependencies": {
    "vue": "^2.6.6",
    "core-js": "^2.6.5",
    "element-ui": "^2.7.2"
  },
  "devDependencies": {
    "vue-template-compiler": "^2.5.21",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-plugin-babel": "^3.5.0",
    "is-my-json-valid": "^2.19.0",
    "eslint-plugin-vue": "^5.0.0",
    "@vue/cli-service": "^3.5.0",
    "babel-eslint": "^10.0.1",
    "sass-loader": "^7.1.0",
    "node-sass": "^7.0.0",
    "eslint": "^5.8.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "files": [
    "dist/*.*"
  ],
  "homepage": "https://tggcs.github.io/projects/schema-json-vue/index.html",
  "keywords": [
    "vue",
    "jsonschema",
    "json-schema",
    "form-builder"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tggcs/schema-json-vue/issues"
  }
}
